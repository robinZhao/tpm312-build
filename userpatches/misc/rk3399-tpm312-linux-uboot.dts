        trips {
                cpu_warm: cpu_warm {
                        temperature = <60000>;
                        hysteresis = <2000>;
                        type = "active";
						status = "okay";
                };

                cpu_hot: cpu_hot {
                        temperature = <75000>;
                        hysteresis = <2000>;
                        type = "active";
						status = "okay";
                };
        };

        cooling-maps {
                map3 {
                        trip = <&cpu_warm>;
                        cooling-device = <&fan0 1 5>;
						status = "okay";
                };

                map4 {
                        trip = <&cpu_hot>;
                        cooling-device = <&fan0 6 9>;
						status = "okay";
                };
        };
