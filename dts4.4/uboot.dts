				interrupts = <0x00 0x67 0x04 0x00 0x00 0x68 0x04 0x00 0x00 0x6a 0x04 0x00>;
				status = "okay";
				u-boot,dm-pre-reloc;
			};
		};
	};

	usb@fe800000 {
		#address-cells = <0x02>;
		#size-cells = <0x02>;
		compatible = "rockchip,rk3399-xhci";
		reg = <0x00 0xfe800000 0x00 0x100000>;
